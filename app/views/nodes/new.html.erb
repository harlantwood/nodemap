<h1>New node</h1>

<% form_for(@node, :url => { :action => "create" }) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :key %><br />
    <%= f.text_field :key %>
  </p>
  <p>
    <%= f.label :html %><br />
    <%= f.text_area :html %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>
                                 